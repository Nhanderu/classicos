        script(inline, defer, src="chrome-mobile.js")

        span#teams(data-team-1=team1, data-team-2=team2)
        script(src="https://unpkg.com/axios/dist/axios.min.js")
        script(inline, defer, src="fetch.js")
        script(inline, defer, src="tempdata.js")
        script(inline, defer, src="display.js")
