        meta(name='author' content='Rafael Escobar')
        meta(name='description' content='')
        meta(name='viewport' content='width=device-width, initial-scale=1.0')

        link(inline, rel='stylesheet', href='page.css')
        link(inline, rel='stylesheet', href='util.css')

        title #{title}

    body

        header
            h1 #{title}

